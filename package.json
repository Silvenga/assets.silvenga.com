{
    "name": "assets.silvenga.com",
    "version": "1.0.0",
    "repository": "git@github.com:Silvenga/assets.silvenga.com.git",
    "author": "Mark Lopez <m@silvenga.com>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "parcel build src/**/*.html",
        "postbuild": "cp src/robots.txt dist/robots.txt && cp src/CNAME dist/CNAME",
        "start": "parcel serve --no-autoinstall src/**/*.html"
    },
    "targets": {
        "app": {
            "distDir": "./dist"
        }
    },
    "browserslist": "defaults",
    "devDependencies": {
        "@parcel/transformer-sass": "^2.9.3",
        "parcel": "^2.9.3",
        "sass": "^1.63.6"
    },
    "dependencies": {
        "normalize.css": "^8.0.1",
        "regenerator-runtime": "^0.13.11",
        "typeface-lato": "^1.1.13"
    }
}
